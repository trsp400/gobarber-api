{
  "name": "gobarber-api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev:server": "NODE_ENV=development ts-node-dev --transpile-only --ignore-watch node_modules src/index.ts",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js",
    "typeorm:cli": "ts-node-dev ./node_modules/typeorm/cli -f ./ormconfig.docker.ts",
    "db:migrate": "yarn typeorm:cli migration:run ",
    "migration:revert": "yarn typeorm:cli migration:revert",
    "migration:create": "yarn typeorm:cli migration:create",
    "db:sync": "yarn typeorm:cli schema:sync && yarn db:migrate",
    "db:drop": "yarn typeorm:cli schema:drop"
  },
  "devDependencies": {
    "@types/node": "^14.11.10",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.0.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-prettier": "^3.2.0",
    "prettier": "^2.2.1",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "bcryptjs": "^2.4.3",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "fastify": "^3.6.0",
    "fastify-cors": "^4.1.0",
    "fastify-oas": "^3.0.3",
    "fastify-plugin": "^3.0.0",
    "mysql": "^2.18.1",
    "mysql2": "^2.2.5",
    "pg": "^8.4.2",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^9.0.0",
    "typeorm": "^0.2.29",
    "uuidv4": "^6.2.4"
  }
}
